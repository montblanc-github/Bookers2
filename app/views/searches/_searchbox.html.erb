<% if user_signed_in? %>
    <%= form_with url: search_path, method: :get, local: true do |f| %>
      <%= f.text_field :content %>
      <%= f.select 'model', { "User" => "user", "Post" => "post" } %>
      <%= f.select 'method', { "完全一致" => "perfect", "部分一致" => "partial" } %>
      <%= f.submit '検索' %>
    <% end %>
<% end %>